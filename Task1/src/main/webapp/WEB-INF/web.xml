<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UsersServlet</servlet-name>
  	<display-name>UsersServlet</display-name>
  	<description></description>
  	<servlet-class>UsersServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>News</servlet-name>
  	<display-name>News</display-name>
  	<description></description>
  	<servlet-class>News</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Fetch</servlet-name>
  	<display-name>Fetch</display-name>
  	<description></description>
  	<servlet-class>Fetch</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UsersServlet</servlet-name>
  	<url-pattern>/UsersServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>News</servlet-name>
  	<url-pattern>/News</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Fetch</servlet-name>
  	<url-pattern>/Fetch</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ApiConnection</servlet-name>
  	<url-pattern>/ApiConnection</url-pattern>
  </servlet-mapping>
</web-app>
